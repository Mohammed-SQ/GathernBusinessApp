<?xml version="1.0" encoding="utf-8" ?>
<Shell
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:GathernBusinessApp"
    x:Class="GathernBusinessApp.AppShell"
    Shell.NavBarIsVisible="False"
    Shell.FlyoutBehavior="Flyout"
    >
    
    <ShellContent
        Title="Signinsplash"
        Route="signinsplash"
        ContentTemplate="{DataTemplate local:SignInSplashPage}" />
    
    <ShellContent
        Title="Splash"
        Route="SplashPage"
        ContentTemplate="{DataTemplate local:SplashPage}" />
    
    <ShellContent
        Title="Boarding"
        Route="boarding"
        ContentTemplate="{DataTemplate local:BoardingPage}" />

    <ShellContent
        Title="SignIn"
        Route="signin"
        ContentTemplate="{DataTemplate local:SignInPage}" />
    
    <ShellContent
        Title="الرئيسية"
        Route="MainPage"
        ContentTemplate="{DataTemplate local:MainPage}" />
    
    <ShellContent
        Title="Tutorial"
        Route="TutorialPage"
        ContentTemplate="{DataTemplate local:TutorialPage}" />
    
    <ShellContent
        Title="Add Property"
        Route="AddPropertyPage"
        ContentTemplate="{DataTemplate local:AddPropertyPage}" />

    <ShellContent
        Title="Dashboard"
        Route="DashboardPage"
        ContentTemplate="{DataTemplate local:DashboardPage}" />
</Shell>
